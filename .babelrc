{
	"presets": [
		"es2015"
	],
	"plugins": [
		"add-module-exports",
 		["transform-runtime", {
 			"helpers": true,
 		    "polyfill": false,
 		    "regenerator": true,
 		    "moduleName": "babel-runtime"
 		}],
 		"transform-async-to-generator",
 		"transform-object-rest-spread",
 		["./babel-plugin-try-catch", {
 			"errorHandler": 'stiError'
 		}]
	]
}